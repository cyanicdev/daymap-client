<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8"/>

        <link href="https://fonts.googleapis.com/css?family=Poppins&display=swap" rel="stylesheet"> 
        <style type="text/css">body {
    font-family: 'Poppins', sans-serif;
    min-width: 500px;
    color: white;
    background-color: #0B0E1F;
}

.blurred {
    filter: blur(10px);
    opacity: 0.7;
    pointer-events: none;
}

#login-prompt {
    border-radius: 10px;
    padding: 20px;
    background-color: #0f0e18;
    text-align: center;
    z-index: 10000;
    box-shadow: 0px 0px 5px black;
}

.login-input {
    padding: 10px;
    border: none;
    color: #ddd;
    background-color: #1a1a1a;
    border-radius: 5px;
    margin-bottom: 1rem;
}

.centered {
    display: flex;
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
}

.centered > * {
    margin: auto;
}

#enter-button {
    transition: all 200ms;
    margin: 10px 0 0 0;
    color: #ddd;
    padding: 10px 70px 10px 70px;
    border-radius: 5px;
    border: 0;
    background-color: #222;
}

#enter-button:hover {
    background-color: #333;
    color: white;
    box-shadow: 0px 2px 5px #111;
}</style>
        <style type="text/css">@keyframes shake {
    0% { transform: translate(0px, 0px); }
    20% { transform: translate(10px, 0px); }
    40% { transform: translate(-10px, 0px); }
    60% { transform: translate(10px, 0px); }
    80% { transform: translate(-10px, 0px); }
    100% { transform: translate(0px, 0px); }
}

.shook {
    animation: shake 0.5s;
}</style>

        <script>function gebi(idName) {
    return document.getElementById(idName);
}

function gebc(className) {
    return document.getElementsByClassName(className);
}</script>
        <script>function shake(element) {
    if(element.classList.contains("shook")) {
        return;
    }

    element.classList.add("shook");
    
    setTimeout(function (){
        element.classList.remove("shook");
    }, 500);
}</script>
        <script>function login() {
    shake(gebi("login-prompt"));
}</script>
    </head>
    <body>
        <div class="centered"><div id="login-prompt">
            <input class="login-input" type="text" placeholder="Username">
            <br>
            <input class="login-input" type="password" placeholder="Password">    
            <br>
            <button id="enter-button" onclick="login()">Login</button>
        </div></div>

        <img src="data:image/png;base64,"/>

        <main class="blurred">
            <button>Click Me</button>
            <input type="checkbox">
            <input type="radio">
        </main>
    </body>
</html>